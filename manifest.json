{
   "name": "SourceCheck", 
   "version": "0.1",
   "description": "Rate articles and links for validity.",
   "browser_action": 
   {
      "default_icon": {                    
         "16": "icon/icon16.png",           
         "24": "icon/icon24.png",           
         "32": "icon/icon32.png"            
       }
   },
   "content_scripts": [
       {
           "matches": [ "http://*/*", "https://*/*" ],
           "js": [ "lib/jquery-3.3.1.js", "js/scFront.js" ],
           "run_at": "document_idle"
       }
   ],
   "permissions": [ "tabs" ],
   "icons": {
      "16": "icon/icon48.png",
      "48": "icon/icon48.png",
      "128": "icon/icon128.png"
   },
   "manifest_version": 2
}
